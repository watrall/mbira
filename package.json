{
  "name": "mbira",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "lint": "next lint --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "format": "prettier --check .",
    "format:write": "prettier --write .",
    "prepare": "husky install",
    "storybook": "cross-env PORT=6006 storybook dev --config-dir .storybook",
    "storybook:ci": "cross-env PORT=6006 storybook dev --config-dir .storybook --ci",
    "storybook:build": "cross-env PORT=6006 storybook build --config-dir .storybook",
    "storybook:serve-static": "http-server storybook-static -p 6006 --silent",
    "storybook:test": "npm run storybook:build && start-server-and-test storybook:serve-static http://127.0.0.1:6006 \"test-storybook --url http://127.0.0.1:6006 --config-dir .storybook --ci\"",
    "supabase:typegen": "supabase gen types typescript --schema public --project-id \"$SUPABASE_PROJECT_ID\" > src/types/schema.ts"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@hookform/resolvers": "^5.2.2",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/storage-js": "^2.76.1",
    "@supabase/supabase-js": "^2.76.1",
    "@tanstack/react-query": "^5.90.5",
    "@tanstack/react-query-devtools": "^5.90.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "fuse.js": "^7.1.0",
    "lucide-react": "^0.548.0",
    "next": "14.2.5",
    "react": "^18",
    "react-dom": "^18",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.65.0",
    "slugify": "^1.6.6",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@storybook/addon-a11y": "^8.4.3",
    "@storybook/addon-essentials": "^8.4.3",
    "@storybook/addon-interactions": "^8.4.3",
    "@storybook/addon-themes": "^8.4.3",
    "@storybook/nextjs": "^8.4.3",
    "@storybook/test": "^8.4.3",
    "@storybook/test-runner": "^0.17.0",
    "@tailwindcss/forms": "^0.5.10",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/coverage-v8": "^4.0.4",
    "cross-env": "^10.1.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-tailwindcss": "^3.18.2",
    "eslint-plugin-testing-library": "^7.13.3",
    "http-server": "^14.1.1",
    "husky": "^9.1.7",
    "jsdom": "^27.0.1",
    "lint-staged": "^16.2.6",
    "postcss": "^8",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "start-server-and-test": "^2.1.2",
    "storybook": "^8.4.3",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.{md,json,css,scss,html}": [
      "prettier --write"
    ]
  }
}
